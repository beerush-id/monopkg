{
  "type": "module",
  "name": "tsdown",
  "version": "0.0.1",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "directories": {
    "dist": "dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@eslint/css": "^0.13.0",
    "@eslint/js": "^9.38.0",
    "@eslint/markdown": "^7.5.0",
    "esbuild-raw-plugin": "^0.3.1",
    "eslint": "^9.38.0",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "publint": "^0.3.15",
    "rimraf": "^6.0.1",
    "tsdown": "^0.15.9",
    "typescript-eslint": "^8.46.2"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "scripts": {
    "dev": "tsdown --watch",
    "clean": "rimraf dist",
    "build": "tsdown && publint",
    "prepublish": "tsdown && publint"
  },
  "license": "MIT"
}